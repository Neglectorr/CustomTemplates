# ollamadeepseeknative.kvp
# Dit is het enige KVP-bestand, dat alle configuratie bevat, nu exact gematcht met node.kvp.

Meta.DisplayName=Ollama DeepSeek Console (Native PowerShell)
Meta.Description=Start de DeepSeek LLM via Ollama met een native PowerShell-commando op de host, en behoudt een actieve console.
Meta.OS=Windows
Meta.ArchSupport=Native
Meta.Arch=All
Meta.Author=Neglectorr
Meta.URL=https://github.com/Neglectorr/CustomTemplates
Meta.Category=AI/LLM
Meta.Icon=genericserver
Meta.FriendlyName=Ollama DeepSeek Runner
Meta.MinAMPVersion=2.6.0.0
Meta.ConfigVersion=1.0
Meta.NoCommercialUsage=False
Meta.DockerRequired=False
Meta.DockerBaseReadOnly=False
Meta.ContainerPolicy=Supported
Meta.ContainerPolicyReason=Native Windows PowerShell execution
Meta.Prerequisites=[]
Meta.ExtraContainerPackages=[]
Meta.ConfigReleaseState=Release
Meta.AppConfigId=ollamadeepseeknativeconfig.json
Meta.DisplayImageSource=

# App settings
App.DisplayName=Ollama DeepSeek Console
App.ExecutableWin=C:\Windows\System32\WindowsPowerShell\v1.0\powershell.exe
App.WorkingDir=
App.WindowsCommandLineArgs=-NoProfile -Command "& { Write-Host 'Starting Ollama DeepSeek model...'; ollama run hf.co/unsloth/DeepSeek-R1-0528-Qwen3-8B-GGUF:Q4_K_XL; }"
App.UseLinuxIOREDIR=False
App.ExitMethod=OS_CLOSE
App.ExitTimeout=30
App.ExitString=exit
App.HasWriteableConsole=True
App.HasReadableConsole=True
App.SupportsLiveSettingsChanges=False
App.ForceIPBinding=False
App.ApplicationIPBinding=0.0.0.0
App.Ports=11434/TCP, 11434/UDP
App.PrimaryApplicationPortRef=0
App.MaxUsers=0
App.UseRandomAdminPassword=False
App.PersistRandomPassword=False
App.AdminMethod=STDIO
App.IgnoreSTDOUTAfterRCON=False
App.RCONConnectDelaySeconds=5
App.RCONConnectRetrySeconds=5
App.RCONHeartbeatMinutes=0
App.RCONHeartbeatCommand=ping
App.TelnetLoginFormat={0}
App.PreStartStages=[]
App.UserActions=[]
App.ForceUpdate=False
App.SteamUpdateAnonymousLogin=True
App.SteamForceLoginPrompt=False
App.RapidStartup=False
App.SmartExcludeExemptions=["*.cfg","*.conf","*.config","*.ini","*.json","*.xml","*.properties","*.kvp","*.yml","*.yaml","*.toml","*.lua"]
App.SmartExcludeSupported=False
App.DumpFullChildProcessTree=False
App.MonitorChildProcessName=
App.MonitorDirectChildOnly=False
App.SupportsUniversalSleep=False
App.WakeupMode=Any
App.ApplicationReadyMode=Immediate

# Console settings
Console.FilterMatchRegex=\e\[(\d+;)*(\d+)?[ABCDHJKfmsu]|\e\[?[?\>\=\da-z]+
Console.FilterMatchReplacement=
Console.ThrowawayMessageRegex=
Console.AppReadyRegex=^$
Console.UserJoinRegex=^$
Console.UserLeaveRegex=^$
Console.UserChatRegex=^$
Console.UpdateAvailableRegex=^$
Console.MetricsRegex=
Console.SuppressLogAtStart=False
Console.ActivateLogRegex=
Console.UserActions={}

# Limits settings
Limits.SleepMode=False
Limits.SleepOnStart=False
Limits.SleepDelayMinutes=5
Limits.DozeDelay=2
Limits.AutoRetryCount=2
Limits.SleepStartThresholdSeconds=25